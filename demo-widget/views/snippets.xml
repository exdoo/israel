<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<template id="basic_snippet" name="Basic Snippet">
   
  <div class="footer">
    <div class="page-padding">
      <div class="container-medium">
        <div class="padding-vertical padding-large">
          <div class="simple-footer">
            <a href="#" class="footer-brand w-inline-block"><img src="images/cf-karhu.svg" loading="lazy" alt=""></a>
            <div class="flex-element">
              <a href="https://www.facebook.com/cfkarhu" target="_blank" class="icon-button w-inline-block"><img src="images/facebook.svg" loading="lazy" alt=""></a>
              <a href="https://www.instagram.com/cf.karhu/" target="_blank" class="icon-button w-inline-block"><img src="images/instagram.svg" loading="lazy" alt=""></a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

</template>

<template id="external_snippets" inherit_id="website.snippets" priority="8">
   <xpath expr="//div[@id='snippet_feature']//t[@t-snippet][last()]" position="after">
       <t t-snippet="demo-widget.basic_snippet"/>
   </xpath>
</template>

</odoo>